{
	"useTabs": false,
	"tabWidth": 2,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "css",
	"printWidth": 80,
	"proseWrap": "always",
	"semi": true,
	"singleQuote": false,
	"trailingComma": "es5",
	"overrides": [
		{
			"files": "*.md",
			"options": {
				"tabWidth": 2,
				"useTabs": false
			}
		}
	]
}
